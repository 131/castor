{
  "name": "castor",
  "version": "1.3.3",
  "description": "An ES7 [CAS designed](https://en.wikipedia.org/wiki/Content-addressable_storage) file storage for nodejs with a simple API and robust implementation.",
  "main": "store.js",
  "scripts": {
    "eslint": "eslint .",
    "checkall": "npm run eslint",
    "test": "npm run checkall && npm run cover",
    "mocha": "mocha -b",
    "coveralls": "npm install coveralls && node ./node_modules/coveralls/bin/coveralls.js < ./coverage/lcov.info",
    "cover": "nyc  --temp-directory=coverage/.nyc_output --reporter=text-summary --reporter=html --reporter=lcov  mocha -b"
  },
  "author": "Francois Leurent <131.code@leurent.email>",
  "license": "MIT",
  "dependencies": {
    "debug": "^2.2.0",
    "mout": "^1.1.0",
    "nyks": "^6.8.5",
    "send": "^0.16.2"
  },
  "devDependencies": {
    "eslint-plugin-ivs": "^2.0.3",
    "expect.js": "^0.3.1",
    "mocha": "^5.2.0",
    "nyc": "^15.1.0"
  },
  "directories": {
    "doc": "doc",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/131/castor.git"
  },
  "keywords": [
    "cas",
    "storage",
    "md5"
  ],
  "bugs": {
    "url": "https://github.com/131/castor/issues"
  },
  "homepage": "https://github.com/131/castor#readme"
}
